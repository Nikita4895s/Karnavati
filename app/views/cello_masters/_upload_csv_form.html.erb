<%= form_for CelloMaster.new, url: upload_csv_path, method: :post, remote:true do |f| %>
  <div class="row mt-5">
    <div class="input-group">
      <div class="input-group-prepend">
        <%= f.submit 'Upload', class: 'input-group-text', id: 'inputGroupFileAddon01' %>
      </div>
      <div class="custom-file">
        <%= f.file_field :picture, class: "custom-file-input", id: "inputGroupFile01",
                'aria-describedby': "inputGroupFileAddon01" %>
        <label class="custom-file-label" for="inputGroupFile01">Choose file</label>
      </div>
    </div>
  </div>
<% end %>