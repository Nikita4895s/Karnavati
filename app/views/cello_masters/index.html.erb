<h1>Cello Masters</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th>Divison</th>
      <th>Category</th>
      <th>Product name</th>
      <th>Capacity</th>
      <th>Volume</th>
      <th>Mrp</th>
      <th>Drp</th>
      <th>Trade discount</th>
      <th>Rate</th>
      <th>Scheme</th>
      <th>Net rate</th>
      <th>Product Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="cello_master_list">
    <% @cello_masters.each do |cello_master| %>
      <%= render partial: 'cello_master_row', locals: {cello_master: cello_master}%>
    <% end %>
  </tbody>
</table>

