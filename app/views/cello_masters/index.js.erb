<% if params[:q].present? %>
  $('#cello_master_list').empty();
<% else %>
  <% if @cello_masters.next_page %>
    $('.pagination').replaceWith('<%= j will_paginate @cello_masters %>');
  <% else %>
    $(window).off('scroll');
    $('.pagination').remove();
  <% end %>
<% end %>
<% @cello_masters.each do |cello_master| %>
  $('#cello_master_list').append("<%= j(render partial: 'cello_master_row', locals: {cello_master: cello_master}) %>");
<% end %>
$(".product_img").on("click", function() {
  $('#imagepreview').attr('src', $(this).attr('src'));
  $('#imagemodal').modal('show');
});
